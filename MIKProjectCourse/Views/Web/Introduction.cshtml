        }
    }
</style>

<script>
$(document).ready(function() {
    if (typeof hljs !== 'undefined') {
        hljs.highlightAll();
    }

    $('.copy-btn').on('click', function() {
        const $codeElement = $(this).closest('.code-example').find('code');
        const codeText = $codeElement.text();
        
        navigator.clipboard.writeText(codeText).then(() => {
            const originalText = $(this).html();
            $(this).html('<i class="fas fa-check"></i> Copied!');
            $(this).addClass('btn-success').removeClass('btn-outline-secondary');
            
            setTimeout(() => {
                $(this).html(originalText);
                $(this).removeClass('btn-success').addClass('btn-outline-secondary');
            }, 2000);
        });
    });

    $('.answer-btn').on('click', function() {
        const $button = $(this);
        const selectedAnswer = $button.data('answer');
        const correctAnswer = "A";
        const $feedback = $('.exercise-feedback');

        $('.answer-btn').removeClass('correct incorrect');

        if (selectedAnswer === correctAnswer) {
            $button.addClass('correct');
            $feedback.html(`
                <div class="feedback-content">
                    <i class="fas fa-check-circle feedback-icon"></i>
                    <div class="feedback-text">
                        <strong>Correct!</strong><br>
                        The program adds 5 + 3 = 8. The + operator performs addition when used with numbers.
                    </div>
                </div>
            `).addClass('correct').show();
        } else {
            $button.addClass('incorrect');
            $feedback.html(`
                <div class="feedback-content">
                    <i class="fas fa-times-circle feedback-icon"></i>
                    <div class="feedback-text">
                        <strong>Not quite!</strong><br>
                        Remember: When you use + with numbers, it adds them (5 + 3 = 8). 
                        Only with text does it combine them ("5" + "3" = "53").
                    </div>
                </div>
            `).addClass('incorrect').show();
        }

        $('.answer-btn[data-answer="' + correctAnswer + '"]').addClass('correct');
    });

    $('.application-card').hover(
        function() {
            $(this).css({
                'transform': 'translateY(-5px)',
                'box-shadow': '0 10px 20px rgba(0,0,0,0.15)'
            });
        },
        function() {
            $(this).css({
                'transform': 'translateY(0)',
                'box-shadow': 'none'
            });
        }
    );

    const observerOptions = {
        threshold: 0.1,
        rootMargin: '0px 0px -50px 0px'
    };

    const observer = new IntersectionObserver((entries) => {
        entries.forEach(entry => {
            if (entry.isIntersecting) {
                entry.target.style.opacity = '1';
                entry.target.style.transform = 'translateY(0)';
            }
        });
    }, observerOptions);

    $('.syntax-item').each(function(index) {
        $(this).css({
            'opacity': '0',
            'transform': 'translateY(20px)',
            'transition': 'all 0.5s ease ' + (index * 0.1) + 's'
        });
        observer.observe(this);
    });

    const consoleLines = $('.console-line');
    let currentLine = 0;
    
    function animateConsole() {
        if (currentLine < consoleLines.length) {
            const $line = $(consoleLines[currentLine]);
            $line.css('opacity', '0');
            
            setTimeout(() => {
                $line.animate({opacity: 1}, 800);
            }, currentLine * 800);
            
            currentLine++;
            setTimeout(animateConsole, 1000);
        }
    }

    setTimeout(animateConsole, 2000);
});
</script>